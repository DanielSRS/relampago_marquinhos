{
	"name": "car",
	"version": "0.0.0",
	"license": "MIT",
	"bin": "dist/app.js",
	"type": "module",
	"engines": {
		"node": ">=16"
	},
	"scripts": {
		"build": "tsc",
		"build_and_run": "yarn build && yarn start",
		"dev": "tsc --watch",
		"start": "node dist/apps/car/src/app.js",
		"logs": "node dist/apps/car/src/utils/logServer.js",
		"test": "prettier --check . && xo && ava"
	},
	"files": [
		"dist"
	],
	"dependencies": {
		"ink": "^4.1.0",
		"ink-select-input": "^6.0.0",
		"ink-spinner": "^5.0.0",
		"ink-tab": "^5.1.0",
		"ink-text-input": "^6.0.0",
		"meow": "^11.0.0",
		"react": "^18.2.0"
	},
	"devDependencies": {
		"@sindresorhus/tsconfig": "^3.0.1",
		"@types/react": "^18.0.32",
		"@vdemedes/prettier-config": "^2.0.1",
		"ava": "^5.2.0",
		"chalk": "^5.2.0",
		"eslint-config-xo-react": "^0.27.0",
		"eslint-plugin-react": "^7.32.2",
		"eslint-plugin-react-hooks": "^4.6.0",
		"ink-testing-library": "^3.0.0",
		"prettier": "^2.8.7",
		"ts-node": "^10.9.1",
		"typescript": "^5.0.3",
		"xo": "^0.53.1"
	},
	"ava": {
		"extensions": {
			"ts": "module",
			"tsx": "module"
		},
		"nodeArguments": [
			"--loader=ts-node/esm"
		]
	},
	"xo": {
		"extends": "xo-react",
		"prettier": true,
		"rules": {
			"react/prop-types": "off"
		}
	},
	"prettier": {
		"arrowParens": "avoid",
		"bracketSameLine": true,
		"bracketSpacing": true,
		"singleQuote": true,
		"trailingComma": "all"
	}
}
