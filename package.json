{
  "name": "relempago_marquinhos",
  "main": "src/main.ts",
  "type": "module",
  "private": true,
  "workspaces": {
    "packages": [
      "apps/*"
    ]
  },
  "scripts": {
    "start": "node --experimental-strip-types src/main.ts",
    "test": "vitest",
    "check": "tsc",
    "lint": "eslint .",
    "runcar": "yarn workspace car build_and_run",
    "runstation": "node --no-warnings --experimental-strip-types src/station.ts",
    "runserver": "node --experimental-strip-types --watch src/server.ts"
  },
  "dependencies": {
    "@types/node": "^22.13.14",
    "react-native-logs": "^5.3.0",
    "typescript": "^5.8.2",
    "zod": "3.24.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.23.0",
    "@react-native/eslint-config": "0.77.1",
    "eslint": "^9.23.0",
    "globals": "^16.0.0",
    "prettier": "^3.5.3",
    "typescript-eslint": "^8.28.0",
    "vitest": "^3.0.8"
  },
  "prettier": {
    "arrowParens": "avoid",
    "bracketSameLine": true,
    "bracketSpacing": true,
    "singleQuote": true,
    "trailingComma": "all"
  },
  "packageManager": "yarn@4.6.0"
}
